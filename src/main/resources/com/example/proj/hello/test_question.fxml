      </VBox>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
         <items>
            <Button fx:id="prev" mnemonicParsing="false" onAction="#prev" text="Предыдущий" />
            <Button fx:id="next" mnemonicParsing="false" onAction="#next" text="Следующий" />
            <Button fx:id="list" mnemonicParsing="false" onAction="#toQuestionList" text="К списку вопросов" />
         </items>
      </ToolBar>
   </children>
